<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codeitsuisse.team92.expensetracker" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MyMaterialTheme" >
        <activity
            android:name=".Sign_Up"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".Main_Screen"
            android:label="@string/title_activity_main__screen" >
        </activity>
        <activity
            android:name=".Pre_launch_activity"
            android:label="Team 92" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Pass_Check"
            android:label="@string/title_activity_pass__check" >
        </activity>
        <activity
            android:name=".Add_Transcation"
            android:label="@string/title_activity_add__transcation" >
        </activity>
        <activity
            android:name=".Edit_data"
            android:label="@string/title_activity_edit_data" >
        </activity>
        <activity
            android:name=".Date_Select"
            android:label="@string/title_activity_date__select" >
        </activity>
        <activity
            android:name=".Display_Main"
            android:label="@string/title_activity_display__main" >
        </activity>
        <activity
            android:name=".Add_Income"
            android:label="@string/title_activity_add__income" >
        </activity>
        <activity
            android:name=".Income_Show"
            android:label="@string/title_activity_add__income" >
        </activity>
        <activity
            android:name=".Budget_Saving"
            android:label="@string/title_activity_budget__saving" >
        </activity>

        <receiver
            android:name=".OnBootListener"
            android:enabled="true"
            android:exported="true" >
        </receiver>

        <service
            android:name=".ExpenseManager"
            android:enabled="true"
            android:exported="true" >
        </service>

        <activity
            android:name=".Show_Liability"
            android:label="@string/title_activity_show__liability" >
        </activity>
    </application>

</manifest>
